<?xml version="1.0" encoding="UTF-8"?>
<manifest>
  <default sync-j="4" revision="thud"/>

  <remote name="tm"    fetch="ssh://git@github.com/sr105-tm"/>
  <remote name="digi"  fetch="git://github.com/digi-embedded"/>
  <remote name="fsl"   fetch="git://github.com/Freescale"/>
  <remote name="qt5"   fetch="git://github.com/meta-qt5"/>
  <remote name="swu"   fetch="git://github.com/sbabic"/>
  <remote name="oe"    fetch="git://git.openembedded.org"/>
  <remote name="yocto" fetch="git://git.yoctoproject.org"/>

  <project name="meta-tm-bsp.git"       path="sources/meta-tm-bsp"       remote="tm"    revision="thud" sync-c="true"/>
  <project name="meta-tm-sw.git"        path="sources/meta-tm-sw"        remote="tm"    revision="thud" sync-c="true">
      <linkfile src="conf/README.md" dest="README.md"/>
  </project>
  <project name="meta-digi.git"         path="sources/meta-digi"         remote="digi"  revision="thud" sync-c="true"/>
  <project name="meta-freescale.git"    path="sources/meta-freescale"    remote="yocto" revision="thud" sync-c="true"/>
  <project name="meta-fsl-demos.git"    path="sources/meta-fsl-demos"    remote="fsl"   revision="thud" sync-c="true"/>
  <project name="meta-openembedded.git" path="sources/meta-openembedded" remote="oe"    revision="thud" sync-c="true"/>
  <project name="meta-qt5.git"          path="sources/meta-qt5"          remote="qt5"   revision="thud" sync-c="true"/>
  <project name="meta-swupdate.git"     path="sources/meta-swupdate"     remote="swu"   revision="thud" sync-c="true"/>
  <project name="poky.git"              path="sources/poky"              remote="yocto" revision="thud" sync-c="true"/>
</manifest>
